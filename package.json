{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "Aplikasi restful-api dengan studi kasus Authentication dan Authorization menggunakan Expressjs dan Typescript",
  "main": "server.js",
  "scripts": {
    "ts-compile": "rm -rf build/ && tsc",
    "ts-watch": "rm -rf build/ && tsc -w",
    "dev": "concurrently \"npm run ts-watch\" \"nodemon ./build/server.js\""
  },
  "keywords": [
    "Nodejs",
    "Expressjs",
    "Typescript"
  ],
  "author": "Andry Pebrianto",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "concurrently": "^6.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.13",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mysql2": "^2.3.0",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0"
  }
}
